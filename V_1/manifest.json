  {
    "manifest_version": 3,
    "name": "Text to Image - Phi2",
    "version": "1.0",
    "description": "Transform web text into captivating illustrations using AI.",
    "permissions": [
      "contextMenus",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "background": {
      "service_worker": "background.js"
    }
  }